            <parameter datatype="string"  min_n_values="1"  max_n_values="1"  name="class_instances_pretty_name"  default="Class" description="used to display everywhere in the ui"/>
            <parameter datatype="string"  min_n_values="1"  max_n_values="1"  name="class_instances_pretty_plural"  default="Classes" description="used everywhere in the ui"/>
